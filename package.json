{
  "name": "xentinet-vulnerability-scanner",
  "version": "1.0.0",
  "description": "Multi-source vulnerability scanner for smart contracts",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "scan": "tsx src/cli.ts",
    "simulator": "tsx src/cli-simulator.ts",
    "simulate": "tsx src/cli-simulator.ts simulate",
    "organize": "tsx src/cli-simulator.ts organize",
    "api-server": "tsx src/api-server.ts",
    "api-server:build": "npm run build && node dist/api-server.js"
  },
  "keywords": [
    "security",
    "vulnerability",
    "scanner",
    "smart-contracts",
    "solana",
    "defi"
  ],
  "author": "Xentinet",
  "license": "MIT",
  "dependencies": {
    "@coral-xyz/anchor": "^0.30.0",
    "@solana/spl-token": "^0.4.14",
    "@solana/web3.js": "^1.87.6",
    "axios": "^1.6.2",
    "chalk": "^5.3.0",
    "cheerio": "^1.0.0-rc.12",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "ethers": "^6.9.0",
    "express": "^4.18.2",
    "glob": "^10.3.10",
    "ora": "^5.4.1",
    "simple-git": "^3.20.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
